{
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base", "plugin:vue/essential"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": ["vue"],
  "rules": {
    "semi": "off",
    "quotes": "off",
    "no-tabs": "off",
    "max-len": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "comma-dangle": "off"
  }
}
